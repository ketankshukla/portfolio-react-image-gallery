{
  "name": "vanilla-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@tailwindcss/postcss7-compat": "^2.0.2",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.2",
    "autoprefixer": "^9.8.6",
    "axios": "^0.21.1",
    "navi": "^0.14.0",
    "postcss": "^7.0.35",
    "postcss-cli-simple": "^3.1.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hookedup": "^0.1.2",
    "react-icons": "^4.1.0",
    "react-navi": "^0.14.4",
